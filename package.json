{
  "name": "go-go-gatsby",
  "private": true,
  "description": "A Gatsby blog with Netlify CMS.",
  "version": "0.1.0",
  "author": "Brittney Postma <brittneypostma@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-mdx": "^1.2.17",
    "gatsby-plugin-netlify-cms": "^4.3.6",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-postcss": "^2.3.6",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-sharp": "^2.5.5",
    "netlify-cms-app": "^2.12.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "tailwindcss": "^1.4.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css -w",
    "build": "npm run build:css && gatsby build",
    "develop": "npm run watch:css & gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
